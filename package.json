{
    "name": "nr-log-server",
    "version": "0.0.1",
    "description": "nr-log-server",
    "main": "index.js",
    "scripts": {
        "start": "npm run build && cd dist && node app.js",
        "start-debug": "npm run build && cd dist && node --inspect --max-old-space-size=8192 app.js",
        "start-debug-brk": "npm run build && cd dist && node --inspect-brk --max-old-space-size=8192 app.js",
        "build": "node tools/pre-build.js && npm run prepare && node tools/copy.js",
        "publish": "cd ./dist && npm publish",
        "pretest": "tsc",
        "test": "nyc mocha dist/test",
        "test-debug": "nyc mocha --inspect-brk dist/test",
        "watch": "mocha-typescript-watch",
        "prepare": "tsc"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nikolay-rogovoy/nr-log-server.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/nikolay-rogovoy/nr-log-server/issues"
    },
    "homepage": "https://github.com/nikolay-rogovoy/nr-log-server#readme",
    "devDependencies": {
        "@types/chai": "4.1.4",
        "@types/mocha": "^5.2.5",
        "chai": "^4.1.2",
        "mocha": "^5.2.0",
        "mocha-typescript": "^1.1.17",
        "nyc": "^12.0.0",
        "source-map-support": "^0.5.6",
        "ts-node": "^3.3.0",
        "typescript": "^3.0.1",
        "ncp": "^2.0.0",
        "mkdirp": "^0.5.1",
        "rimraf": "^2.6.2"
    },
    "nyc": {
        "check-coverage": true,
        "lines": 80,
        "statements": 80,
        "functions": 80,
        "branches": 80,
        "include": [
            "dist/**/*.js"
        ],
        "exclude": [
            "test/**/*.js"
        ],
        "reporter": [
            "lcov",
            "text-summary"
        ],
        "all": true
    },
    "dependencies": {
        "@types/express": "^4.16.0",
        "@types/mongoose": "^5.2.3",
        "@types/node": "^10.5.4",
        "@types/winston": "^2.3.9",
        "express": "^4.16.3",
        "jsonwebtoken": "^8.3.0",
        "mongoose": "^5.2.6",
        "nconf": "^0.10.0",
        "nr-log-parser": "^0.0.5",
        "reflect-metadata": "^0.1.12",
        "rxjs": "^5.5.1",
        "MD5": "~1.3.0",
        "winston": "^2.4.3"
    }
}
